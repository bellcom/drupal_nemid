{
  "name": "signaturgruppen/spsclient-php",
  "description": "PHP client for NemID ServiceProvider (TU) Services",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.signaturgruppen.dk:platformspakke/spsclient-php-api.git"
    }
  ],
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Signaturgruppen A/S",
      "email": "info@signaturgruppen.dk"
    }
  ],
  "require": {
    "netresearch/jsonmapper": "v0.11.0",
    "signaturgruppen/spsclient-php-api": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.4.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "archive": {
    "exclude": [
      "tests"
    ]
  }
}
